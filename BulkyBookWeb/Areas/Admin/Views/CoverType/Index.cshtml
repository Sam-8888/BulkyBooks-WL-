@model IEnumerable<CoverType>

@{
    ViewData["Title"] = "Index";
}
<partial name="_Notification" />
<div class="container p-3">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Cover Type List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="CoverType" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i>
                &nbsp; Create New Cover Type</a>
        </div>
    </div>

<table class="table table-bordered table-striped">

    <thead>
        <tr>
            <th>Name</th>
          
            <th></th>
        </tr>
    </thead>
    <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="50%">@obj.Name</td>
                    <td>
                        <a class="mx-2" asp-controller="CoverType" asp-action="Edit" asp-route-id="@obj.Id"><i class="bi bi-pencil-square"></i></a>
                        <a class="mx-2" asp-controller="CoverType" asp-action="Delete" asp-route-id="@obj.Id"><i class="bi bi-trash-fill"></i></a>
                    </td>
                </tr>
            }
    </tbody>
</table>
</div>


